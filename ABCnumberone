#include <isotream>
#include <windows.h>
using namespace std;
int main() {
 SetConsoleCP(1251);
 SetConsoleOutputCP(1251);
 int chislo;
 cout << "Введите число в двоичной системе счисления: ";
 cin >> chislo;
 if (chislo & 1)
  cout << "Нечётное" << endl;
 else
  cout << "Чётное" << endl;
 return 0;
}
